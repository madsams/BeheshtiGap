<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="pane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerSetting">
   <center>
      <VBox alignment="TOP_CENTER" prefWidth="300.0" spacing="30.0" styleClass="pane" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER_LEFT" spacing="20.0">
               <children>
                  <ImageView fx:id="image" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
                  <Label fx:id="name" text="Label" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <JFXButton onAction="#changeImage" prefWidth="300.0" styleClass="uncolored-background" text="Change Image" />
            <JFXButton layoutX="20.0" layoutY="180.0" onAction="#deleteImage" prefWidth="300.0" styleClass="uncolored-background" text="Delete Image" />
            <JFXButton onAction="#changeUser" prefWidth="300.0" styleClass="uncolored-background" text="Change Username" />
            <JFXButton layoutX="65.0" layoutY="171.0" onAction="#changePass" prefWidth="300.0" styleClass="uncolored-background" text="Change Password" />
            <JFXButton layoutX="20.0" layoutY="363.0" onAction="#logout" prefWidth="300.0" styleClass="uncolored-background" text="Logout" />
            <JFXButton layoutX="20.0" layoutY="424.0" onAction="#back" prefWidth="300.0" styleClass="uncolored-background" text="Back" />
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets bottom="40.0" left="10.0" right="10.0" top="20.0" />
         </padding>
      </VBox>
   </center>
</BorderPane>
